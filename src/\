use std::io;

fn main() -> io::Result<()>{
    let p: vec2 = vec2::new(32,32);
    Ok(())
}

fn rl() -> String{
    let mut input = String::new();
    io::stdin().read_line(&mut input);
    return input;
}

struct vec2 {
    x: i32,
    y: i32
}

impl vec2 {
    fn new(x: i32, y:i32) -> vec2{
        return vec2{ x: x, y: y};
    }
}
